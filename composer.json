{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-gd": "^7.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "artesaos/seotools": "^0.20.2",
        "beyondcode/laravel-comments": "^1.2",
        "fideloper/proxy": "^4.0",
        "haruncpi/laravel-user-activity": "^1.0",
        "intervention/image": "^2.4",
        "jordanmiguel/laravel-popular": "1.0.10",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "munafio/chatify": "^1.3.4",
        "overtrue/laravel-follow": "^3.0",
        "overtrue/laravel-like": "^4.0",
        "rachidlaasri/laravel-installer": "^4.1",
        "sightengine/client-php": "dev-master",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-pjax": "^2.1",
        "staudenmeir/eloquent-eager-limit": "^1.6",
        "tanmuhittin/laravel-google-translate": "^2.0",
        "yajra/laravel-datatables": "^1.5",
        "yoelpc4/laravel-cloudinary": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-query-detector": "^1.5",
        "facade/ignition": "^2.10",
        "fakerphp/faker": "^1.14",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:itsrafsanjani/sightengine-client-php.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
